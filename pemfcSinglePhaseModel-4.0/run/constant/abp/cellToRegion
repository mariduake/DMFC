FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.001/abp";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    abpSides
    {
        type            zeroGradient;
    }
    abpLeft
    {
        type            zeroGradient;
    }
    abp_to_fuel
    {
        type            calculated;
        value           uniform 0;
    }
}


