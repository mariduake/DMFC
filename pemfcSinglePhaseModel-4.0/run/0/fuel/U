FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fuel";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           uniform (0 0 1.1055);
    }

    fuelOutlet
    {
        type            zeroGradient;
    }

    fuelSides
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    agdlSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
    
    aclSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
        
    fuel_to_electrolyte //anode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    fuel_to_abp
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

